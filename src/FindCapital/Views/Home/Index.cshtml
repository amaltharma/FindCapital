@{
    ViewData["Title"] = "Home Page";
}

<div style="padding: 12px 20px; margin: 8px 0;">
    <form>
        <div class="form-group row">
            <label for="StateName" class="col-sm-2 form-control-label">State Name</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="stateName" placeholder="">
            </div>
        </div>
        <div class="form-group row">
            <label for="StateNameAbbrivation" class="col-sm-2 form-control-label">State Name Abbrivation</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="stateNameAbbrivation" placeholder="">
            </div>
        </div>
        <div class="form-group row">
            <label for="StateCapitol" class="col-sm-2 form-control-label">State Capitol</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="stateCapitol" placeholder="">
            </div>
        </div>
    </form>
</div>



